<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeepWise.Test"
        xmlns:UI="clr-namespace:DeepWise.Controls;assembly=dw_base"
        xmlns:AccessControls="clr-namespace:DeepWise.AccessControls;assembly=dw_base" x:Class="DeepWise.Test.DictionaryViewDemo"
        mc:Ignorable="d"
        Title="PropertyGridDemo" 
        UseLayoutRounding="True"
        SnapsToDevicePixels="True"
        Height="800"
        Width="600"
        WindowStartupLocation="CenterScreen" x:Name="this">
    <Grid x:Name="stackpnl">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <UI:DictionaryView x:Name="dictionaryView" IsSmallButtonVisiable="True" SmallButtonClicked="dictionaryView_SmallButtonClicked">

        </UI:DictionaryView>
    </Grid>
</Window>
